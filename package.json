{
  "name": "easywebpack-vue",
  "version": "3.0.0",
  "description": "vue webpack building solution for easywebpack",
  "keywords": [
    "webpack",
    "easywebpack",
    "vue"
  ],
  "dependencies": {
    "easywebpack": "^3.0.0",
    "eslint-plugin-vue": "^2.1.0",
    "vue": "^2.4.2",
    "vue-hot-reload-api": "^2.1.0",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.0.4",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2"
  },
  "devDependencies": {
    "axios": "^0.16.2",
    "chai": "^4.1.1",
    "cross-env": "^5.0.5",
    "eslint": "^4.5.0",
    "eslint-config-eslint": "^4.0.0",
    "mocha": "^3.5.0",
    "vue-router": "^2.7.0",
    "vuex": "^2.3.1",
    "vuex-router-sync": "^4.2.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production node test/build",
    "build-dev": "cross-env NODE_ENV=development node test/build",
    "build-client": "cross-env BUILD_ENV=client npm run build",
    "build-server": "cross-env BUILD_ENV=server npm run build",
    "build-client-dev": "cross-env BUILD_ENV=client npm run build-dev",
    "build-server-dev": "cross-env BUILD_ENV=server npm run build-dev",
    "start": "cross-env NODE_SERVER=true NODE_ENV=development node test/build",
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "test": "mocha test",
    "ci": "npm run lint && npm run cov",
    "ii": "npm install --registry https://registry.npm.taobao.org"
  },
  "files": [
    "index.js",
    "lib",
    "config",
    "tool"
  ],
  "ci": {
    "version": "6, 7, 8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hubcarl/easywebpack-vue.git"
  },
  "bugs": {
    "url": "https://github.com/hubcarl/easywebpack-vue/issues"
  },
  "homepage": "https://github.com/hubcarl/easywebpack-vue#readme",
  "author": "hubcarl@126.com",
  "license": "MIT"
}
